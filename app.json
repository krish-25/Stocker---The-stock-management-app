{
  "expo": {
    "name": "stocker",
    "slug": "stocker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ganpati-foils-logo.png",
    "splash": {
      "image": "./assets/Green-patta-ganesha.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ganpatifoils.stocker",
      "enableDangerousExperimentalLeanBuilds": true,
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/ganpati-foils-logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/ganpati-foils-logo.png"
    }
  }
}
